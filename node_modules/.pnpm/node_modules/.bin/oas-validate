#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/xsrm/stripe-to-zod/node_modules/.pnpm/swagger2openapi@7.0.8/node_modules/swagger2openapi/node_modules:/home/xsrm/stripe-to-zod/node_modules/.pnpm/swagger2openapi@7.0.8/node_modules:/home/xsrm/stripe-to-zod/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/xsrm/stripe-to-zod/node_modules/.pnpm/swagger2openapi@7.0.8/node_modules/swagger2openapi/node_modules:/home/xsrm/stripe-to-zod/node_modules/.pnpm/swagger2openapi@7.0.8/node_modules:/home/xsrm/stripe-to-zod/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../swagger2openapi/oas-validate.js" "$@"
else
  exec node  "$basedir/../swagger2openapi/oas-validate.js" "$@"
fi
